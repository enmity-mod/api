{
  "name": "enmity-api",
  "description": "Api wrapper for Enmity.",
  "version": "1.0.8",
  "license": "GPL-3.0",
  "scripts": {
    "build": "tsc",
    "docs": "typedoc --readme none --entryPointStrategy expand ./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enmity-mod/enmity-api.git"
  },
  "bugs": {
    "url": "https://github.com/enmity-mod/enmity-api/issues"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.11.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.5.4"
  },
  "exports": {
    "./assets": {
      "types": "./dist/assets.d.ts",
      "import": "./dist/assets.js"
    },
    "./clyde": {
      "types": "./dist/clyde.d.ts",
      "import": "./dist/clyde.js"
    },
    "./commands": {
      "types": "./dist/commands.d.ts",
      "import": "./dist/commands.js"
    },
    "./common": {
      "types": "./dist/common.d.ts",
      "import": "./dist/common.js"
    },
    "./modules": {
      "types": "./dist/modules.d.ts",
      "import": "./dist/modules.js"
    },
    "./native": {
      "types": "./dist/native.d.ts",
      "import": "./dist/native.js"
    },
    "./patcher": {
      "types": "./dist/patcher.d.ts",
      "import": "./dist/patcher.js"
    },
    "./plugins": {
      "types": "./dist/plugins.d.ts",
      "import": "./dist/plugins.js"
    },
    "./settings": {
      "types": "./dist/settings.d.ts",
      "import": "./dist/settings.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "assets": [
        "./dist/assets.d.ts"
      ],
      "clyde": [
        "./dist/clyde.d.ts"
      ],
      "commands": [
        "./dist/commands.d.ts"
      ],
      "common": [
        "./dist/common.d.ts"
      ],
      "modules": [
        "./dist/modules.d.ts"
      ],
      "native": [
        "./dist/native.d.ts"
      ],
      "patcher": [
        "./dist/patcher.d.ts"
      ],
      "plugins": [
        "./dist/plugins.d.ts"
      ],
      "settings": [
        "./dist/settings.d.ts"
      ]
    }
  }
}
